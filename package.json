{
  "name": "jonesbrothers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "CHOKIDAR_USEPOLLING=1 next dev",
    "dev:clean": "lsof -ti:3000 | xargs kill -9 2>/dev/null; sleep 1; npm run dev",
    "dev:safe": "CHOKIDAR_USEPOLLING=1 next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && rm -rf docs && cp -r out docs && touch docs/.nojekyll && cp docs/index.txt docs/jonesbrothers.txt"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "next": "^14.2.5"
  },
  "devDependencies": {
    "typescript": "^5.5.3",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5"
  }
}
